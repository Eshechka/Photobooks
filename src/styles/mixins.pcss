@mixin desktopHd() {
  @media screen and (min-width: 1800px) {
    @content; 
  }
}

@mixin desktop() {
  @media screen and (min-width: 1200px) {
    @content;
  }
}

@mixin tablets() {
  @media screen and (min-width: 768ps) {
    @content;
  }
}

@mixin phones() {
  @media screen and (min-width: 480px) {
    @content;
  }
}